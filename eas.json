{
  "cli": {
    "version": ">= 16.18.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NODE_ENV": "development",
        "GOOGLE_SERVICES_JSON": "@file:GOOGLE_SERVICES_JSON",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      },
      "android": {
        "gradleCommand": ":app:assembleDebug"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "NODE_ENV": "staging",
        "GOOGLE_SERVICES_JSON": "@file:GOOGLE_SERVICES_JSON",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      },
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "NODE_ENV": "production",
        "GOOGLE_SERVICES_JSON": "@file:GOOGLE_SERVICES_JSON",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      },
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    }
  },
  "submit": {
    "production": {}
  }
}